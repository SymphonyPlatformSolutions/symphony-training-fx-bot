<div style="border-left:3px solid purple;">
    <table style='margin-top:5px;margin-left:5px;margin-bottom:0px;width:200px;'>
        <thead>
        <tr>
            <td>FROM</td>
            <td>TO</td>
            <td>RATE</td>
        </tr>
        </thead>
        <tbody>
        {{#each this}}
            <tr>
                <td style="border:0px"><span style="font-size:14px">{{from}}</span></td>
                <td style="border:0px"><span style="font-size:14px">{{to}}</span></td>
                <td style="border:0px"><b><span style="font-size:18px">{{rate}}</span></b><span style="font-size:10px;margin-left:3px">{{to}}</span></td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>